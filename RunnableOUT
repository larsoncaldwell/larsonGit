import javax.swing.*;

public class RunnableOUT
{
   JButton toStop;
   JList threadsOUT;
   JLabel labelOUT;

   private RunnableOUT()
   {
      toStart = new JButton("Stop");
      threadsIN = new JList();
      labelIN = new JLabel("These are the running applications");
      toStop.addActionListener(this);
   }

   public RunnableOUT getInstance()
   {
      return this;
   }
   
   public void actionPerformed(ActionEvent event)
   {
      button.setText("Its Over!");
   }
}
