public class Advent
{
   public static void main(String[] args)
   {
      new Advent().start();
   }

   public void start()
   {
      RustySword rustSword = new RustySword();
      MagicWand theWand = new MagicWand();

      use(rustSword);
   }

   public void use(Sword theSword)
   {
      for(int i = 0; i < 5; i++)
      {
         theSword.attack();
      }
      theSword.putAway();
   }

   public interface Sword
   {
      public void attack();
      public void putAway();
   }

   public interface Wand
   {
      public void cast();
      public void putAway();
   }

   public class RustySword
      implements Sword
   {
      public void attack()
      {
         System.out.println("Klank!");
      }
      
      public void putAway()
      {
         System.out.println("Shing!");
      }
   }

   public class MagicWand
      implements Wand
   {
      public void cast()
      {
         System.out.println("Alacazam!");
      }
      
      public void putAway()
      {
         System.out.println("Poof!");
      }
   }
}
